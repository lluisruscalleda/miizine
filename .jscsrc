{
  "preset": "airbnb",
  "validateLineBreaks": null,
  "maxErrors": null,
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "excludeFiles": [
    "node_modules/**",
    "coverage/*",
    "client/*"
  ],
  "maximumLineLength": { "value": 120, "allExcept": [ "functionSignature", "urlComments", "regex", "comments", "require"] }
}

